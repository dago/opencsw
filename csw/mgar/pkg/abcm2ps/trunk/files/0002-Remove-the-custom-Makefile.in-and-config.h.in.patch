-dist: Changes
-	ln -s . abcm2ps-$(VERSION); \
-	tar -zcvf abcm2ps-$(VERSION).tar.gz $(DIST_FILES); \
-	rm abcm2ps-$(VERSION)
-
-zip-dist:
-	ln -s . abcm2ps-$(VERSION); \
-	zip -r abcm2ps-$(VERSION).zip $(DIST_FILES); \
-	rm abcm2ps-$(VERSION)
-
-zip: abcm2ps.exe
