-
-srcdir = @srcdir@
-VPATH = @srcdir@
-bindir = @bindir@
-libdir = @libdir@
-datadir = @datarootdir@
-docdir = @docdir@
-
-# unix
-OBJECTS=abc2ps.o \
-	abcparse.o buffer.o deco.o draw.o format.o front.o glyph.o music.o parse.o \
-	slre.o subs.o svg.o syms.o
-abcm2ps: $(OBJECTS)
