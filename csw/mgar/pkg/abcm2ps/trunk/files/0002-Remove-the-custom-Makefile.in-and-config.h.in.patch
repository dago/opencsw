index d9f01fe..0000000
--- a/Makefile.in
+++ /dev/null
@@ -1,148 +0,0 @@
-# Makefile source for abcm2ps
-
-VERSION = @VERSION@
-
-CC = @CC@
-INSTALL = @INSTALL@
-INSTALL_DATA = @INSTALL_DATA@
-INSTALL_PROGRAM = @INSTALL_PROGRAM@
-
-CPPFLAGS = @CPPFLAGS@ -I.
-CPPPANGO = @CPPPANGO@
