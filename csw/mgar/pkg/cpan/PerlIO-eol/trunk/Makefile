NAME = PerlIO-eol
VERSION = 0.14
CATEGORIES = cpan
GARTYPE = v2
AUTHOR = AUDREYT

DESCRIPTION = PerlIO layer for normalizing line endings
define BLURB
  This layer normalizes any of CR, LF, CRLF and Native into the designated
  line ending. It works for both input and output handles.
endef

PACKAGES = CSWpmperlioeol
CATALOGNAME = pm_perlioeol

include gar/category.mk
