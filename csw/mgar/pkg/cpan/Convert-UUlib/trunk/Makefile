GARNAME = Convert-UUlib
GARVERSION = 1.33
CATEGORIES = cpan
AUTHOR = MLEHMANN

DESCRIPTION = Perl interface to the uulib library
define BLURB
  Convert::UUlib is a versatile and powerful decoder/encoder library
  for a variety of encodings used in Usenet and Mail (uuencode,
  xxencode, b64, binhex...).
endef

PACKAGES = CSWpmconvertuulib
CATALOGNAME = pm_convertuulib

include gar/category.mk
