NAME = String-Approx
VERSION = 3.26
CATEGORIES = cpan
GARTYPE = v2
AUTHOR = JHI

DESCRIPTION = Approximate matching (fuzzy matching)
define BLURB
  String::Approx lets you match and substitute strings approximately. 
  With this you can emulate errors: typing errorrs, speling errors, 
  closely related vocabularies (colour color), genetic mutations (GAG ACT), 
  abbreviations (McScot, MacScot).
endef

PACKAGES = CSWpmstringapprox
CATALOGNAME = pm_stringapprox

include gar/category.mk
