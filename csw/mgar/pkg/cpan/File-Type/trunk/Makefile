NAME = File-Type
VERSION = 0.22
CATEGORIES = cpan
GARTYPE = v2
AUTHOR = PMISON

DESCRIPTION = determine file type using magic
define BLURB
  File::Type uses magic numbers (typically at the start of a file) to
  determine the MIME type of that file.
endef

DISTFILES = $(call admfiles,CSWpmfiletype,depend prototype)

