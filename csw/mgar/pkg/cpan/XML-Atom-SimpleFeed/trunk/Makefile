GARNAME = XML-Atom-SimpleFeed
GARVERSION = 0.86
CATEGORIES = cpan
AUTHOR = ARISTOTLE

DESCRIPTION = No-fuss generation of Atom syndication feeds
define BLURB
  This module provides a minimal API for generating Atom syndication
  feeds quickly and easily. It supports all aspects of the Atom format,
  but has no provisions for generating feeds with extension elements.
endef

PACKAGES = CSWpmxmlatomsimplefd
CATALOGNAME = pm_xmlatomsimplefeed
ARCHALL = 1

LICENSE = readme

include gar/category.mk
