NAME = OOTools
VERSION = 2.21
CATEGORIES = cpan
GARTYPE = v2
AUTHOR = DOMIZIO

DESCRIPTION = Easily create constructors and lvalue accessors
define BLURB
	OOTools is a collection of pragmas to easily create constructors methods and
	lvalue accessor methods at compile time.
endef

