NAME = X11-Protocol
VERSION = 0.56
CATEGORIES = cpan
AUTHOR = SMCCAM

DESCRIPTION = Perl module for the X Window System Protocol, version 11
define BLURB
  X11::Protocol is a client-side interface to the X11 Protocol, allowing
  perl programs to display windows and graphics on X11 servers.
endef

LICENSE = README

PACKAGES += CSWpm-x11-protocol
SPKG_DESC_CSWpm-x11-protocol = $(DESCRIPTION)
OBSOLETED_BY_CSWpm-x11-protocol += CSWpmx11protocol

ARCHALL_CSWpm-x11-protocol = 1

include gar/category.mk
