#!/bin/sh

smf=no
if [ -f /usr/sbin/svccfg -a -f /usr/sbin/svcadm ] ;
then smf=yes ; fi

if [ $smf = yes ]
  then
  # Disable service
     /usr/sbin/svcadm disable -s svc:system/cswdbus:default >/dev/null 2>&1

   # Unregister with SMF
   /usr/sbin/svccfg delete svc:system/cswdbus:default >/dev/null 2>&1
fi
