%include        url file://%{GARDIR}/pkglib/csw_vars.gspec
%pkginfo        url file://%{GARDIR}/pkglib/csw/pkginfo
%prototype      exec %{GARBIN}/cswproto -s %{TIMESTAMP} -v basedir=%{DESTDIR} %{DESTDIR}%{prefix}=%{prefix}
%depend         url file://%{GARDIR}/pkglib/csw/depend.perl
%depend         exec awk '$1 ~ /f/{print $3}' %{prototype} | %{GARBIN}/depmaker --root %{DESTDIR} --nodep %{pkgname} %{DEPMAKER_EXTRA_ARGS}
