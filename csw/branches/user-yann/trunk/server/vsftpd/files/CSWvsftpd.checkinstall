trap `exit 3` 15
# Assign Solaris release class to install release specific files
smf=no
if [ -f $BASEDIR/usr/sbin/svccfg -a -f $BASEDIR/usr/sbin/svcadm ] ; then smf=yes ; fi
if [ $smf = yes ]
then
        CLASSES='none smfyes'
else
        CLASSES='none smfno'
fi

# Make env variables available to other packaging scripts

cat >$1 <<!
CLASSES='$CLASSES'
!

exit 0
