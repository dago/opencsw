GARNAME = XML-AutoWriter
GARVERSION = 0.38
CATEGORIES = cpan
AUTHOR = RBS

DESCRIPTION = DOCTYPE driven XML output
define BLURB
  This module uses the DTD contained in an XML::Doctype to enable compile- and
  run-time checks of XML output validity. It also provides methods and
  functions named after the elements mentioned in the DTD.
endef

DISTFILES = $(call admfiles,CSWpmxmlautowriter,depend prototype)

include ../category.mk
