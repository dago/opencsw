trap `exit 3` 15
set -x
# Assign Solaris release class to install release specific files
if [ -f "$BASEDIR/usr/sbin/svccfg" -a -f "$BASEDIR/usr/sbin/svcadm" ]; then
        CLASSES="$CLASSES smf"
else
        CLASSES="$CLASSES init"
fi

# Make env variables available to other packaging scripts
cat >$1 <<!
CLASSES=$CLASSES
!

exit 0
